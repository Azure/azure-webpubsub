{
  "name": "@azure/webpubsub",
  "author": "Microsoft Corporation",
  "description": "WebPubSubServiceClient Library with typescript type definitions for node.js and browser.",
  "version": "1.0.0",
  "dependencies": {
    "@azure/core-http": "^1.2.2",
    "@azure/ms-rest-azure-js": "^2.0.1",
    "@azure/ms-rest-js": "^2.0.4",
    "cloudevents": "^4.0.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "node-fetch": "^2.6.1",
    "tslib": "^1.10.0",
    "typescript-base64-arraybuffer": "^0.1.0",
    "ws": "^7.4.3"
  },
  "keywords": [
    "node",
    "azure",
    "typescript",
    "browser",
    "isomorphic"
  ],
  "license": "MIT",
  "main": "./dist/webpubsub.js",
  "module": "./esm/webPubSubServiceClient.js",
  "types": "./esm/webPubSubServiceClient.d.ts",
  "devDependencies": {
    "@types/express": "^4.17.11",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/node": "^14.14.25",
    "dotenv": "^8.2.0",
    "rollup": "^1.18.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "typescript": "^3.5.3",
    "uglify-js": "^3.6.0"
  },
  "homepage": "https://github.com/Azure/azure-sdk-for-js",
  "repository": {
    "type": "git",
    "url": "https://github.com/Azure/azure-sdk-for-js.git"
  },
  "bugs": {
    "url": "https://github.com/Azure/azure-sdk-for-js/issues"
  },
  "files": [
    "dist/**/*.js",
    "dist/**/*.js.map",
    "dist/**/*.d.ts",
    "dist/**/*.d.ts.map",
    "esm/**/*.js",
    "esm/**/*.js.map",
    "esm/**/*.d.ts",
    "esm/**/*.d.ts.map",
    "src/**/*.ts",
    "README.md",
    "rollup.config.js",
    "tsconfig.json"
  ],
  "scripts": {
    "build": "tsc && rollup -c rollup.config.js && npm run minify",
    "minify": "uglifyjs -c -m --comments --source-map \"content='./dist/webpubsub.js.map'\" -o ./dist/webpubsub.min.js ./dist/webpubsub.js",
    "prepack": "npm install && npm run build"
  },
  "sideEffects": false
}
