{
    "name": "scoreboard",
    "version": "0.1.0",
    "private": true,
    "main": "dist/app.js",
    "scripts": {
        "dev:all": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
        "dev:server": "nodemon --config ./src/server/nodemon.config.json",
        "dev:client": "set NODE_ENV=development && webpack serve --progress --hot --config ./src/client/webpack.config.js",
        "build:client": "set NODE_ENV=production && webpack --config ./src/client/webpack.config.js",
        "build:server": "set NODE_ENV=production && webpack --config ./src/server/webpack.config.js",
        "build:all": "concurrently \"npm run build:server\" \"npm run build:client\"",
        "pack:zip": "npm-build-zip --source ./dist/",
        "test": "echo TODO"
    },
    "dependencies": {
        "@azure/web-pubsub": "^1.0.0",
